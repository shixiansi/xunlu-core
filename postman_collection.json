                    "raw": "{{baseUrl}}/plugins/reset-qianyu-plugin/schedules/{{groupId}}",
                    "protocol": "http",
                    "host": [
                        "localhost"
                    ],
                    "port": "3000",
                    "path": [
                        "plugins",
                        "reset-qianyu-plugin",
                        "schedules",
                        "{{groupId}}"
                    ]
                }
            }
        },
        {
            "name": "Reset Qianyu - Create Schedule",
            "request": {
                "method": "POST",
                "header": [
                    {
                        "key": "Content-Type",
                        "value": "application/json"
                    }
                ],
                "body": {
                    "mode": "raw",
                    "raw": "{ \"type\": \"daily\", \"hour\": 9, \"minute\": 0, \"enabled\": 1, \"days\": [1,2,3] }"
                },
                "url": {
                    "raw": "{{baseUrl}}/plugins/reset-qianyu-plugin/schedules/{{groupId}}",
                    "protocol": "http",
                    "host": [
                        "localhost"
                    ],
                    "port": "3000",
                    "path": [
                        "plugins",
                        "reset-qianyu-plugin",
                        "schedules",
                        "{{groupId}}"
                    ]
                }
            }
        },
        {
            "name": "Reset Qianyu - Delete Schedule",
            "request": {
                "method": "DELETE",
                "header": [],
                "url": {
                    "raw": "{{baseUrl}}/plugins/reset-qianyu-plugin/schedules/{{id}}",
                    "protocol": "http",
                    "host": [
                        "localhost"
                    ],
                    "port": "3000",
                    "path": [
                        "plugins",
                        "reset-qianyu-plugin",
                        "schedules",
                        "{{id}}"
                    ]
                }
            }
        },
        {
            "name": "Reset Qianyu - Top Images",
            "request": {
                "method": "GET",
                "header": [],
                "url": {
                    "raw": "{{baseUrl}}/plugins/reset-qianyu-plugin/images/top/{{groupId}}?limit=10",
                    "protocol": "http",
                    "host": [
                        "localhost"
                    ],
                    "port": "3000",
                    "path": [
                        "plugins",
                        "reset-qianyu-plugin",
                        "images",
                        "top",
                        "{{groupId}}"
                    ],
                    "query": [
                        {
                            "key": "limit",
                            "value": "10"
                        }
                    ]
                }
            }
        },
        {
            "name": "Reset Qianyu - Get Settings",
            "request": {
                "method": "GET",
                "header": [],
                "url": {
                    "raw": "{{baseUrl}}/plugins/reset-qianyu-plugin/settings/{{groupId}}",
                    "protocol": "http",
                    "host": [
                        "localhost"
                    ],
                    "port": "3000",
                    "path": [
                        "plugins",
                        "reset-qianyu-plugin",
                        "settings",
                        "{{groupId}}"
                    ]
                }
            }
        },
        {
            "name": "Reset Qianyu - Set Repeat",
            "request": {
                "method": "POST",
                "header": [
                    {
                        "key": "Content-Type",
                        "value": "application/json"
                    }
                ],
                "body": {
                    "mode": "raw",
                    "raw": "{ \"enabled\": 1 }"
                },
                "url": {
                    "raw": "{{baseUrl}}/plugins/reset-qianyu-plugin/settings/repeat/{{groupId}}",
                    "protocol": "http",
