            "request": {
                "method": "POST",
                "header": [
                    {
                        "key": "Content-Type",
                        "value": "application/json"
                    }
                ],
                "body": {
                    "mode": "raw",
                    "raw": "{ \"url\": \"https://example.com/video\" }"
                },
                "url": {
                    "raw": "{{baseUrl}}/plugins/reset-qianyu-plugin/parse",
                    "protocol": "http",
                    "host": [
                        "localhost"
                    ],
                    "port": "3000",
                    "path": [
                        "plugins",
                        "reset-qianyu-plugin",
                        "parse"
                    ]
                }
            }
        },
        {
            "name": "Reset Qianyu - Download",
            "request": {
                "method": "POST",
                "header": [
                    {
                        "key": "Content-Type",
                        "value": "application/json"
                    }
                ],
                "body": {
                    "mode": "raw",
                    "raw": "{ \"url\": \"https://example.com/video\" }"
                },
                "url": {
                    "raw": "{{baseUrl}}/plugins/reset-qianyu-plugin/download",
                    "protocol": "http",
                    "host": [
                        "localhost"
                    ],
                    "port": "3000",
                    "path": [
                        "plugins",
                        "reset-qianyu-plugin",
                        "download"
                    ]
                }
            }
        },
        {
            "name": "Reset Qianyu - List Downloads",
            "request": {
                "method": "GET",
                "header": [],
                "url": {
                    "raw": "{{baseUrl}}/plugins/reset-qianyu-plugin/downloads",
                    "protocol": "http",
                    "host": [
                        "localhost"
                    ],
                    "port": "3000",
                    "path": [
                        "plugins",
                        "reset-qianyu-plugin",
                        "downloads"
                    ]
                }
            }
        },
        {
            "name": "Reset Qianyu - Get Downloaded File",
            "request": {
                "method": "GET",
                "header": [
                    {
                        "key": "x-download-token",
                        "value": "{{downloadToken}}"
                    }
                ],
                "url": {
                    "raw": "{{baseUrl}}/plugins/reset-qianyu-plugin/downloads/{{fileName}}",
                    "protocol": "http",
                    "host": [
                        "localhost"
                    ],
                    "port": "3000",
                    "path": [
                        "plugins",
                        "reset-qianyu-plugin",
                        "downloads",
                        "{{fileName}}"
                    ]
                }
            }
        }
    ],
    "variable": [
        {
            "key": "baseUrl",
            "value": "http://localhost:3000"
        },
        {
            "key": "groupId",
            "value": "myGroupId"
        },
        {
            "key": "id",
            "value": "123"
        },
        {
            "key": "fileName",
            "value": "your-file.mp4"
        },
        {
            "key": "downloadToken",
            "value": ""
        }
    ]
