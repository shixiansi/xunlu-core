        },
        {
            "name": "Reset Qianyu - Set Config",
            "request": {
                "method": "POST",
                "header": [
                    {
                        "key": "Content-Type",
                        "value": "application/json"
                    }
                ],
                "body": {
                    "mode": "raw",
                    "raw": "{ \"prefix\": \"!\", \"suffix\": \"\" }"
                },
                "url": {
                    "raw": "{{baseUrl}}/plugins/reset-qianyu-plugin/config/{{groupId}}",
                    "protocol": "http",
                    "host": [
                        "localhost"
                    ],
                    "port": "3000",
                    "path": [
                        "plugins",
                        "reset-qianyu-plugin",
                        "config",
                        "{{groupId}}"
                    ]
