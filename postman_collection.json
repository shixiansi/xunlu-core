{
    "info": {
        "name": "plugin-api",
        "_postman_id": "plugin-api-collection",
        "description": "Postman collection for plugin-api routes (use {{baseUrl}} as base)",
        "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
    },
    "item": [
        {
            "name": "Health",
            "request": {
                "method": "GET",
                "header": [],
                "url": {
                    "raw": "{{baseUrl}}/health",
                    "protocol": "http",
                    "host": [
                        "localhost"
                    ],
                    "port": "3000",
                    "path": [
                        "health"
                    ]
                }
            }
        },
        {
            "name": "Bot Event",
            "request": {
                "method": "POST",
                "header": [
                    {
                        "key": "Content-Type",
                        "value": "application/json"
                    }
                ],
                "body": {
                    "mode": "raw",
                    "raw": "{ \"type\": \"message\", \"payload\": { \"text\": \"hello\" } }"
                },
                "url": {
                    "raw": "{{baseUrl}}/bot/event",
                    "protocol": "http",
                    "host": [
                        "localhost"
                    ],
                    "port": "3000",
                    "path": [
                        "bot",
                        "event"
                    ]
                }
            }
        },
        {
            "name": "Example Plugin - Info",
            "request": {
                "method": "GET",
                "header": [],
                "url": {
                    "raw": "{{baseUrl}}/plugins/example/info",
                    "protocol": "http",
                    "host": [
                        "localhost"
                    ],
                    "port": "3000",
                    "path": [
                        "plugins",
                        "example",
                        "info"
                    ]
                }
            }
        },
        {
            "name": "Example Plugin - Echo",
            "request": {
                "method": "POST",
                "header": [
                    {
                        "key": "Content-Type",
                        "value": "application/json"
                    }
                ],
                "body": {
                    "mode": "raw",
                    "raw": "{ \"msg\": \"hello example\" }"
                },
                "url": {
                    "raw": "{{baseUrl}}/plugins/example/echo",
                    "protocol": "http",
                    "host": [
                        "localhost"
                    ],
                    "port": "3000",
                    "path": [
                        "plugins",
                        "example",
                        "echo"
                    ]
                }
            }
        },
        {
